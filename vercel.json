{
    "version": 2,
    "buildVersion": 1,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
}
